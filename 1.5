#include<bits/stdc++.h>
using namespace std;
#define endl "\n"
#define int long long
int mod = 1e9+7;

void solve(int n)
{
	if(n<=5)
		cout<<"1";
	else if(n%5==0)
		cout<<n/5;
	else
		cout<<(n/5+1);
}
int32_t main()
{
    ios_base::sync_with_stdio(false);cin.tie(0);cout.tie(0);
    int n;
    cin>>n;
    solve(n);
    
    return 0;
}
